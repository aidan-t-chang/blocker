{
  "name": "BunnyBlocker",
  "description": "A smart app blocker. All roads lead to Rome.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
        "css": ["style.css"],
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "settings.html",
    "default_icon": "rome.png"
  }

}